    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ASOpSlsUl1 = tssum(ASOpSlsUTmp, current - IClndl1, current - IClndl1)</value>
      </attribute>
    </data_model>
  </data_model>
