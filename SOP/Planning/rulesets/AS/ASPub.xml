  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>ASPub_renovate_2</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>ITmp = if(indexfirst([clnd].[mnth]) &lt; CSWpPlanStartI, CSWpPlanStartI - indexfirst([clnd].[mnth]), 0)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
