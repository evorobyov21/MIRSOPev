  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>PPApp_PPWpPubNegativeB</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>PPWpPubNegativeB=PPWpPublishB&amp;&amp;PPWpCheckNegB</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>PPApp_PPWpCanPubB</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>PPWpCanPubB=if(PPWpPublishB.or==true&amp;&amp;PPWpPubNegativeB.or==false,true,false)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>PPApp_PPWpPublishingB</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>PPWpPublishingB=if(PPWpCanPubB==true&amp;&amp;PPWpPublishB==true,true,false)</value>
      </attribute>
    </data_model>
  </data_model>
  <data_model type="com.retek.ride.rule.newmodel.Rule">
    <attribute name="name">
      <value>PPP_PPWpMsgPubT</value>
    </attribute>
    <attribute name="description">
      <value>a rule</value>
    </attribute>
    <attribute name="creator">
      <value>user</value>
    </attribute>
    <data_model type="java.lang.String">
      <attribute name="name">
        <value>E0</value>
      </attribute>
      <attribute name="formula">
        <value>PPWpMsgPubT=if(PPWpCanPubB,"Опубликование плана успешно завершено",if(PPWpPublishB.or==false,"Ошибка публикации! Для опубликования плана необходимо поставить галочку в мере РОПП Публикация РП, да/нет",if(PPWpPubNegativeB.or,"Ошибка публикации! В публикуемом плане найдены отрицательные значения","Ошибка публикации")))</value>
      </attribute>
    </data_model>
  </data_model>
